{
  "expo": {
    "name": "Hackbank",
    "slug": "Hackbank",
    "description": "An app that allows you to simulate an interbank transfer.",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.hackbank.app"
    },
    "android": {
      "package": "io.hackbank.app",
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#006C4B"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
